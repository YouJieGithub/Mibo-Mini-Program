/* pages/Purchases/Purchases.wxss */
.Interestedmodels{
  padding: 0 24rpx;
  margin-top: 35rpx;
  font-size: 20rpx;
}
.section{
  display: flex;
  justify-content: space-around;
  border-bottom: dashed 1rpx #858585;
  line-height: 60rpx;
  background:url('data:image/png;base64,') no-repeat center right;
  background-size: 15rpx 29rpx;
}
.section__title{
  width: 30%;
}
.section__title text{
  color: #999999;
}
picker{
  width: 70%;
  display: flex;
  justify-content: space-between;
}
.picker text{
  padding: 0 100rpx;
}
.ScheduleCarPurchases{
  margin: 41rpx 24rpx 0 24rpx;
  font-size: 20rpx;
  border-bottom: dashed 1rpx #333333;
}
.radio-group{
  padding: 24rpx 0;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.radio .wx-radio-input{
  display: none;
}
.radiot{
  width: 150rpx;
	line-height: 41rpx;
	border: solid 1rpx #333333;
  text-align: center;
}
.radiof{
  display: inline-block;
  width: 150rpx;
	line-height: 41rpx;
	border: solid 1rpx #4ea5f2;
  text-align: center;
}
.radio-checked{
  border: solid 1rpx #cccccc;
  background: red;
}
/* 付款方式 */
.paymentMethod{
  margin-top: 35rpx;
  padding: 0 24rpx;
  font-size: 20rpx;
}
.paymentMethod view{
  display: flex;
  justify-content: space-around;
  align-items: center;
  border-bottom: dashed 1rpx #333333;
}
.paymentMethod view view{
  margin-top: 30rpx;
   margin-bottom: 34rpx;
  text-align: center;
  width: 150rpx;
	height: 41rpx;
	border: solid 1rpx #333333;
}
.section-region{
  margin: 0 24rpx;
  margin-top: 20rpx;
  padding: 18rpx 0;
  display: flex;
  justify-content: space-between;
  font-size: 20rpx;
   background:url('data:image/png;base64,') no-repeat center right;
  background-size: 15rpx 29rpx;
  border-bottom: dashed 1rpx #333333;
}
.section-region picker{
  display: flex;
  flex-direction: column;
  align-items: flex-end;
}
.picker-region{
  padding: 0 100rpx;
}
