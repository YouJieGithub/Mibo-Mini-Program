/* 提交按钮 */
.Submit{
  position:fixed;
  bottom: 10rpx;
}
.Submit button{
  width: 750rpx;
	line-height: 79rpx;
  border-radius: 0;
  font-size: 30rpx;
  font-weight: bolder;
  color: #ffffff;
	background-color: #4ea5f2;
}
