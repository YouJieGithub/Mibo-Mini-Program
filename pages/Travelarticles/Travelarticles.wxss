/* pages/Travelarticles/Travelarticles.wxss */
.Traveldown-article{
  padding: 30rpx 22rpx;
  padding-bottom: 0;
  margin-bottom: 59rpx;
  display: flex;
  justify-content: space-around;
}
.Traveldown-article view:nth-of-type(1){
  height: 146rpx;
}
.Traveldown-article view:nth-of-type(1) image{
  width: 148rpx;
	height: 146rpx;
}
.Traveldown-article view:nth-of-type(2){
  margin-left: 18rpx;
  display: flex;
  flex-direction: column;
}
.Traveldown-article view:nth-of-type(2) text:nth-of-type(1){
  text-align: left;
  font-size: 22rpx;
  font-weight: bolder;
  color: #333333;
}
.Traveldown-article view:nth-of-type(2) text:nth-of-type(2){
    text-indent: 20rpx;
  	font-size: 20rpx;
}
.Traveldown-article view:nth-of-type(2) text:nth-of-type(3){
  text-align: right;
  	font-size: 20rpx;
}